{
  "name": "screeps-typescript-starter",
  "version": "3.0.0",
  "description": "",
  "main": "index.js",
  "//": "If you add or change the names of destinations in screeps.json, make sure you update these scripts to reflect the changes",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "lint-report": "nyc --reporter=lcov --reporter=text-summary eslint \"src/**/*.ts\"",
    "build": "rollup -c",
    "build-report": "nyc --reporter=lcov --reporter=text-summary rollup -c",
    "ma": "rollup -c --environment DEST:main",
    "ps": "rollup -c --environment DEST:pserver",
    "sim": "rollup -c --environment DEST:sim",
    "test": "nyc npm run test && nyc npm run test-integration",
    "test-build": "nyc npm run test-integration-build && nyc npm run test-unit-build",
    "test-integration-nyc": "nyc npm run test-integration",
    "test-unit": "rollup -c rollup.test-unit-config.js && mocha dist/test-unit.bundle.js",
    "test-unit-build": "rollup -c rollup.test-unit-config.js",
    "test-integration-build": "npm run build && rollup -c rollup.test-integration-config.js",
    "test-integration": "npm run build && rollup -c rollup.test-integration-config.js && mocha dist/test-integration.bundle.js",
    "test-integration-report": "nyc --reporter=lcov --reporter=text-summary npm run test-unit",
    "watch-main": "rollup -cw --environment DEST:main",
    "watch-pserver": "rollup -cw --environment DEST:pserver",
    "watch-sim": "rollup -cw --environment DEST:sim",
    "mocha": "mocha dist/test-unit.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/screepers/screeps-typescript-starter.git"
  },
  "author": "",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/screepers/screeps-typescript-starter/issues"
  },
  "homepage": "https://github.com/screepers/screeps-typescript-starter#readme",
  "engines": {
    "node": "10.x || 12.x"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/chai": "^4.2.14",
    "@types/lodash": "3.10.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^13.13.32",
    "@types/screeps": "^3.2.1",
    "@types/sinon": "^5.0.5",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@typescript-eslint/typescript-estree": "^3.10.1",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "lodash": "^3.10.1",
    "mocha": "^5.2.0",
    "prettier": "^2.2.0",
    "rollup": "^2.33.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-cpy": "^2.0.1",
    "rollup-plugin-nodent": "^0.2.2",
    "rollup-plugin-screeps": "^1.0.1",
    "rollup-plugin-typescript2": "^0.27.3",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.2.0",
    "tern": "^0.24.3",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@screeps/common": "^2.15.0",
    "nyc": "^15.1.0",
    "react": "^17.0.1",
    "screeps-profiler": "^2.0.1",
    "screeps-server-mockup": "^1.5.1",
    "source-map": "~0.6.1",
    "ts-toolbelt": "^6.15.5"
  }
}
